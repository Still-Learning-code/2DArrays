//chp 7 2d Arrays
// 2D Arrays: Array of arrays

fun main(args: Array<String>) {

    val matrix2 = Array(3) {
        IntArray(3)
    }
    val matrix = arrayOf(
        intArrayOf(1, 2, 3),//row 0
        intArrayOf(4, 5, 6),//row 1
        intArrayOf(7, 8, 9), //row 2
    )
println(matrix.contentDeepToString())
    println(matrix [1] [1])
    println(matrix[2] [1])
    val rowSize = matrix.size
    val colSize = matrix[0].size

    println (rowSize)
    println (colSize)

 }
// 2D Arrays rows before columns rows -> columns vertical arr[row] [col]
